{
  "name": "splitmate-api",
  "version": "1.0.0",
  "description": "Splitmate API",
  "main": "index.js",
  "scripts": {
    "start": "node ./src/index.js",
    "dev": "env-cmd -e dev nodemon ./src/index.js",
    "prod": "env-cmd -e prod nodemon ./src/index.js",
    "test": "jest --runInBand --detectOpenHandles --collect-coverage",
    "test-local": "env-cmd -e test jest --runInBand --detectOpenHandles"
  },
  "nodemonConfig": {
    "ignore": [
      "snapshot/*"
    ]
  },
  "author": "Roger Floriano (petruki)",
  "dependencies": {
    "apollo-server-express": "^3.8.2",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "express": "^4.18.1",
    "express-validator": "^6.14.1",
    "graphql": "^15.8.0",
    "helmet": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.5.0",
    "mongoose": "^6.3.5",
    "multer": "^1.4.5-lts.1",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1",
    "switcher-client": "^3.1.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.17.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^28.1.0",
    "jest-sonar-reporter": "^2.0.0",
    "nodemon": "^2.0.16",
    "sinon": "^14.0.0",
    "supertest": "^6.2.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trackerforce/splitmate-api"
  }
